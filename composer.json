{
    "name": "arxy/graphql-bundle",
    "type": "bundle",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "webonyx/graphql-php": "~v14.11",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/dependency-injection": "*",
        "symfony/config": "*",
        "symfony/http-kernel": "*",
        "symfony/finder": "*",
        "symfony/property-access": "*",
        "psr/http-factory": "*"
    },
    "require-dev": {
        "symfony/security": "*"
    },
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Arxy\\GraphQL\\": "src/"
        }
    },
    "suggest": {
        "symfony/security": "Authorize prior calling resolvers"
    }
}
